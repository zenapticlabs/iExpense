{
  "expo": {
    "name": "pfu-app",
    "slug": "pfu-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pfu.us.ricoh.expense.app"
    },
    "assetBundlePatterns": [
      "assets/images/*"
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "scopePath": "/",
      "favicon": "./assets/images/brand.png",
      "manifest": {
        "display": "standalone",
        "start_url": "/index.html",
        "scope": "/",
        "icons": [
          {
            "src": "./assets/images/icon.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any maskable"
          }
        ]
      }
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5b6c9476-005e-4123-93cb-6f390fbfcc6a"
      }
    },
    "owner": "loi.go1220"
  }
}